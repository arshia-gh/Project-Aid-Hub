    "chart.js": "2.9.4",
    "classnames": "2.3.1",
    "formik": "^2.2.9",
    "moment": "^2.29.1",
    "node-sass": "6.0.1",
    "node-sass-package-importer": "5.3.2",
    "nouislider": "15.2.0",
    "react": "17.0.2",
    "react-chartjs-2": "2.11.1",
    "react-copy-to-clipboard": "5.0.3",
    "react-datetime": "3.1.1",
    "react-dom": "17.0.2",
    "react-dropzone": "^12.0.4",
    "react-hook-form": "^7.27.1",
    "react-query": "^3.34.19",
    "react-router-dom": "^6.2.2",
    "react-scripts": "4.0.3",
    "react-select": "^5.2.2",
    "reactstrap": "8.9.0",
    "yup": "^0.32.11"
  },
